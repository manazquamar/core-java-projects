package com.coll;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;


public class MainClass {

	public static void main(String[] args) {
		Employee el=new Employee("abhi",40000 );
		Employee e2=new Employee("ashwin",18000 );
		Employee e3=new Employee("ujwal",80000 );
		Employee e4=new Employee("aryan", 19000);
		Employee e5=new Employee("manaz",30000 );
		
		List<Employee> list= new ArrayList<>();
		
		list.add(el);
		list.add(e2);
		list.add(e3);
		list.add(e4);
		list.add(e5);
		
		System.out.println(Collections.max(list,(x,y)-> x.getSal()-y.getSal());
		
		
		
	}

}
